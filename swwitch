<html>
<body>
<script>
var marks = 80;

if (marks >= 100) {
    document.write("A");
} else if (marks >= 69) {
    document.write("B");
} else if (marks >= 59) {
    document.write("C");
} else if (marks >= 49) {
    document.write("D");
} else if (marks >= 39) {
    document.write("fail");
} else {
    document.write("Fail");
}
</script>
</body>
</html>
