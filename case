case(marks >= 69):
    alert("B");
    break;
case(marks >= 59):
    alert("C");
    break;
case(marks >= 49):
    alert("D");
    break;
case(marks >= 39):
    alert("fail");
    break;
deafult:
    alert("Fail");
    break;
}
</script>
